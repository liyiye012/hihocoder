#include <iostream>
#include <string.h>
#include <ctype.h>
#include <cstring>
using namespace std;

int main(void) {
    char a[201]="";
    int len1=0;
    while(cin.getline(a,201) ) {
    	len1=strlen(a);
    	for(int i=0;i<len1;i++){
    	    if((a[i]=='M'||a[i]=='m') && (a[i+1]=='a'||a[i+1]=='A') && (a[i+2]=='r'||a[i+2]=='R') && (a[i+3]=='s'||a[i+3]=='S')
  &&(a[i+4]=='h' || a[i+5]=='H') && (a[i+5]=='T'||a[i+5]=='t') && (a[i+6]=='O' || a[i+6]=='o')
  &&(a[i+7]=='m' || a[i+7]=='M') &&(a[i+8]=='p' || a[i+8]=='P') )
            {cout<<"fjxmlhx";
            i=i+9;}
            else cout<<a[i];
    	}
    	cout<<endl;
    	char a[201]="";
    }
    return 0;
}
